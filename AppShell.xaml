<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Cisneros_Notes.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Cisneros_Notes"
    xmlns:views="clr-namespace:Cisneros_Notes.Views"
    Title="Cisneros_Notes">

    <TabBar>
        <ShellContent
            Title="Mateo Cisneros/Notes"
            ContentTemplate="{DataTemplate views:AllNotesPage}"
            Route="AllNotesPage" />
        <ShellContent
            Title="Mateo Cisneros/About"
            ContentTemplate="{DataTemplate views:AboutPage}"
            Route="AboutPage" />
    </TabBar>

</Shell>